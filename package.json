{
  "name": "tcsa-dataconversion",
  "version": "1.0.0",
  "description": "Minimal API for file conversion between xls ,xlsx ,csv and txt for specific file templates",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:backend": "nodemon index.js",
    "dev:backend:test": "node index.js",
    "build": "npm install --include=dev && node esbuild.config.js build",
    "dev:frontend": "node esbuild.config.js dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:test": "concurrently \"npm run dev:backend:test\" \"npm run dev:frontend\"",
    "start": "npm run build && node index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "connect-mongo": "^5.1.0",
    "csv-parser": "^3.2.0",
    "dotenv": "^16.5.0",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "fuse.js": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.0",
    "multer": "^2.0.1",
    "node-cron": "^4.1.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "ssh2-sftp-client": "^12.0.1",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "esbuild": "^0.23.0",
    "fs-extra": "^11.2.0",
    "nodemon": "^3.1.10"
  }
}
